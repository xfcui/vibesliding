# VibeSliding Configuration Template
# Copy this file to .env and set your actual values
# Priority: --api-key > OPENROUTER_API_KEY env > .env

[openrouter]
# Your OpenRouter API key (required)
# Get one at: https://openrouter.ai/
api_key = your-openrouter-api-key-here

# Optional: SOCKS5 proxy for API requests
# Comment out or remove if not using a proxy
proxy = socks5://127.0.0.1:1080

# AI model to use for image generation
# Default: google/gemini-3-pro-image-preview
# See available models at: https://openrouter.ai/models
model = google/gemini-3-pro-image-preview

# Maximum concurrent API requests
# Higher = faster but uses more resources
# Recommended range: 16-36
max_concurrent = 36
